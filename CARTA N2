#include <stdio.h>

int main() {
    int carta = 2;
    char* codigo = "B";
    char* estado = "guarulhos"; // Declarar como string (ponteiro char)
    char* cidade_nome = "sao paulo"; // Declarar como string (ponteiro char)
    long populacao = 96248723; // Use long para grandes números populacionais
    double area = 7255.99;
    double pib = 8593.56; // PIB em bilhões de reais
    int numeros_de_pontos_turisticos = 145; // Corrigido o nome da variável e adicionado ponto e vírgula

    // Printing the values to check the result
    printf("Carta: %d\n", carta);
    printf("Estado: %s\n", estado);
    printf("Cidade: %s\n", cidade_nome);
    printf("População: %ld\n", populacao);
    printf("Área: %.2f\n", area);
    printf("PIB: %.2f bilhões de reais\n", pib);
    printf("Número de pontos turísticos: %d\n", numeros_de_pontos_turisticos); // Adicionando a impressão dos pontos turísticos

    return 0;
}